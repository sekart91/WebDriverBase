	<repositories>
	   <repository>
	    <id>jenkins-releases</id>
	    <url>http://repo.jenkins-ci.org/releases/</url>
	  </repository>
	  <repository>
    <id>opencast-public</id>
    <url>http://repository.opencastproject.org/nexus/content/repositories/public/</url>
  </repository>
	</repositories>
	<distributionManagement>
	    <repository>
	        <id>repo</id>
	        <url>https://github.com/sekart91/WebDriverBase/tree/master/releases</url>
	    </repository>
	    <snapshotRepository>
	        <id>snapshot-repo</id>
	        <url>https://github.com/sekart91/WebDriverBase/raw/master/snapshots</url>
	    </snapshotRepository>
	</distributionManagement>
</project>